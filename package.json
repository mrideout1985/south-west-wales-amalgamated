{
  "name": "cms-sanity",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "npm run build:sanity && pwd && npm run build:web",
    "build:web": "next build",
    "start:web": "next start",
    "build:sanity": "cd studio && sanity build ../public/studio -y && cd ..",
    "start:sanity": "cp .env ./studio/.env.development && cd studio && sanity start",
    "postinstall": "lerna bootstrap",
    "generate-icons": "npx @svgr/cli -d components/icons --ignore-existing public --ext=js"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^2.21.7",
    "@sanity/image-url": "^1.0.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.15.0",
    "next": "^10.2.0",
    "next-sanity": "^0.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.3.6",
    "react-twitter-widgets": "^1.10.0",
    "react-use": "^17.3.1",
    "swr": "^0.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@sanity/cli": "^2.21.7",
    "@svgr/cli": "^5.5.0",
    "babel-loader": "^8.2.3",
    "babel-preset-react-app": "^10.0.0",
    "css-loader": "^6.4.0",
    "file-loader": "^6.2.0",
    "lerna": "^4.0.0",
    "sass": "^1.43.2",
    "sass-loader": "^12.2.0",
    "style-loader": "^3.3.1"
  }
}
